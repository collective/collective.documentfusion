<?xml version="1.0"?>
<registry>
  <record name="collective.documentfusion.interfaces.ISOfficeSettings.port"
          interface="collective.documentfusion.interfaces.ISOfficeSettings"
          field="port">
    <field type="plone.registry.field.Int">
      <default>8765</default>
      <description>py3o.fusion port</description>
      <required>True</required>
      <title>py3o.fusion port</title>
    </field>
    <value>8765</value>
  </record>
  <record name="collective.documentfusion.interfaces.ISOfficeSettings.host"
          interface="collective.documentfusion.interfaces.ISOfficeSettings"
          field="host">
    <field type="plone.registry.field.ASCIILine">
      <default>localhost</default>
      <description>py3o.fusion host</description>
      <required>True</required>
      <title>py3o.fusion host</title>
    </field>
    <value>localhost</value>
  </record>
</registry>
